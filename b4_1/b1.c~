#include <stdio.h>
#include <stdlib.h>

int main() 
{
	printf("Thuc hien cac lenh:\n");

	system("mkdir -p  ThucHanh1 ThucHanh2");

	system("echo \"Troi hom nay that dep!\" > ThucHanh1/Tho.c");

	system("cp ThucHanh1/Tho.c ThucHanh2/");

	printf("\n---Noi dung tap tin---\n");
	system("cat ThucHanh2/Tho.c");
	printf("---------------------\n");

	return 0;
}
